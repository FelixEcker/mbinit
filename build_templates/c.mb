sector .config
  ; mariebuild c buildscript template from mbinit
  ; author: Marie Eckert

  depends:
    includes ''

  mariebuild:
    binname   ''
    compiler 'gcc'

    files ''

    std_flags     '-Wall $(depends/includes) -c -o'
    debug_flags   '-ggdb'
    release_flags '-O3'

    comp_cmd '$(compiler) $(mode_flags) $(std_flags) out/$(file).o src/$(file)'
